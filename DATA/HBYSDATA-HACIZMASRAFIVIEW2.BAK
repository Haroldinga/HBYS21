	AS SELECT Masraf.Masraf_Id,;
		Masraf.Masraf_fk,;
		Masraf.Muamele_Id,;
		Masraf.Masraf_Tipi,;
		Masraf.Masraf_Tarihi,;
		Masraf.Masraf_Miktari,;
		Masraf.Uygulama_Asamasi,;
		Masraf.Personel_Id,;
		Masraf.Personel_Adi,;
